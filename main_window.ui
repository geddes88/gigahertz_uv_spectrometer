        <property name="borderwidth">10</property>
        <property name="height">200</property>
        <property name="text" translatable="yes">Command Log and Output</property>
        <property name="width">400</property>
        <layout>
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="propagate">True</property>
          <property name="row">4</property>
          <property name="rowspan">1</property>
          <property name="sticky">nsew</property>
        </layout>
        <child>
          <object class="tk.Scrollbar" id="log_out_scrollbar">
            <property name="orient">vertical</property>
            <bind add="" handler="" sequence="" />
            <layout>
              <property name="column">1</property>
              <property name="ipadx">1</property>
              <property name="ipady">71</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
