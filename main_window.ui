        </child>
        <child>
          <object class="ttk.Label" id="utctime_entry">
            <property name="anchor">center</property>
            <property name="justify">center</property>
            <property name="relief">sunken</property>
            <property name="text" translatable="yes">00:00:00</property>
            <property name="width">20</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="localtime_entry">
            <property name="anchor">center</property>
            <property name="relief">sunken</property>
            <property name="text" translatable="yes">00:00:00</property>
            <property name="width">20</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="sza_entry">
            <property name="anchor">center</property>
            <property name="justify">center</property>
            <property name="relief">sunken</property>
            <property name="text" translatable="yes">000.00</property>
            <property name="width">20</property>
            <layout>
              <property name="column">2</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="saz_entry">
